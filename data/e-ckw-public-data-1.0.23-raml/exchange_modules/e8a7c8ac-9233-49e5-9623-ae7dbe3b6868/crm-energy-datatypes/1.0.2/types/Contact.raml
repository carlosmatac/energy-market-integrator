#%RAML 1.0 DataType
displayName: Contact
type: object
properties:
  firstname:
    type: string
    required: true
    example: "Hans"

  lastname:
    type: string
    required: true    
    example: "Muster"

  email:
    type: string
    example: "hans.muster@email.tst"

  mobile:
    type: string
    example: "+41791231212"

  